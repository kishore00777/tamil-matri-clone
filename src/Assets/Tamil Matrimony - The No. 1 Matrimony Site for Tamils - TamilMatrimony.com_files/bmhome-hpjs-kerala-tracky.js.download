function PrivilegefrmValidation() {
    var frm = document.privilegeFrm;
    var specialDigitPattern = /[~`!@#\$%\^&\*()\-_\+=|\\{}\[\]:;"'<>,\?\/\.0123456789]/;
    if (trim(frm.pmInterestedLookingFor.value) == "0") {
        frm.pmInterestedLookingFor.focus();
        alert("Pleae select Seeking Alliance For");
        return false
    }
    if (trim(frm.pmInterestedName.value) == "") {
        frm.pmInterestedName.focus();
        alert("Please enter your Name");
        return false
    } else if (specialDigitPattern.test(frm.pmInterestedName.value)) {
        frm.pmInterestedName.focus();
        frm.pmInterestedName.value =
            "";
        alert("Enter a valid Name");
        return false
    }
    if (trim(frm.pmInterestedMaritalStatus.value) == "0") {
        frm.pmInterestedMaritalStatus.focus();
        alert("Pleae select Marital Status");
        return false
    }
    if (frm.pmInterestedAge.value == "" || frm.pmInterestedAge.value == "Age") {
        frm.DOBDAY.focus();
        if (frm.DOBDAY.value == "0") {
            alert("Please select Date");
            frm.DOBDAY.focus();
            return false
        }
        if (frm.DOBMONTH.value == "0") {
            alert("Please select Month");
            frm.DOBMONTH.focus();
