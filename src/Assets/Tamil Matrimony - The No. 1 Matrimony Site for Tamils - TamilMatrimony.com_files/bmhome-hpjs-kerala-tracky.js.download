function constructCitySelect(data) {
    var i = 1;
    data = eval("(" + data + ")");
    selObj = document.getElementById("pmInterestedCity");
    var length = selObj.options.length;
    i = length;
    for (d in data) selObj.options[i++] = new Option(data[d], d);
    for (i = length; i > 0; i--)
        if (selObj.options[i].value == "otherlocationsinindia" || selObj.options[i].value == "otherlocations") {
            if (selObj.options[i].value == "otherlocations") selObj.options[selObj.options.length] = new Option(selObj.options[i].text, selObj.options[i].value);
            selObj.remove(i)
        }
}

