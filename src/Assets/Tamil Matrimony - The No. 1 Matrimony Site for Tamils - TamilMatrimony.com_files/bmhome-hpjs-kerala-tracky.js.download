function acalchk() {
    var privilegeFrm = this.document.privilegeFrm;
    if (privilegeFrm.DOBDAY.value != 0 && privilegeFrm.DOBMONTH.value != 0) {
        var mchk = privilegeFrm.DOBMONTH.value % 2;
        var ychk = privilegeFrm.DOBYEAR.value % 4;
        if (privilegeFrm.DOBMONTH.value == 2) {
            if (privilegeFrm.DOBDAY.value >= 30) {
                alert("Please select correct Date. This month doesn't have 30 or 31");
                privilegeFrm.DOBDAY.value = 0;
                privilegeFrm.DOBDAY.focus();
                return false
            }
            if (privilegeFrm.DOBDAY.value == 29)
                if (ychk != 0) {
                    alert("This is not a Leap Year. Please select the correct Date");
                    privilegeFrm.DOBDAY.value = 0;
                    privilegeFrm.DOBDAY.focus();
                    return false
                }
        }
        if (privilegeFrm.DOBMONTH.value <= 7 && mchk == 0 || privilegeFrm.DOBMONTH.value >= 8 && mchk == 1)
            if (privilegeFrm.DOBDAY.value == 31) {
                alert("Please select correct Date. This month doesn't have 31");
                privilegeFrm.DOBDAY.value = 0;
                privilegeFrm.DOBDAY.focus();
                return false
            }
    }
}

function aagesel() {
    if (document.getElementById("DOBDAY").value != 0 && document.getElementById("DOBMONTH").value != 0 && document.getElementById("DOBYEAR").value != 0) {
        var agediff = acalculate_age(document.getElementById("DOBYEAR").value, document.getElementById("DOBMONTH").value, document.getElementById("DOBDAY").value);
        document.getElementById("pmInterestedAge").value = agediff
    }
}

function acalculate_age(dobyear, dobmonth, dobday) {
    var diff = "";
    if (dobyear != 0 && dobmonth != 0 && dobday != 0) {
        var dateArrVal = new Array;
        dateArrVal = getTodayDate();
        var today = new Date(dateArrVal["year"], dateArrVal["month"] - 1, dateArrVal["date"]);
        var dob = new Date(dobyear, dobmonth - 1, dobday);
        var one_year = 1E3 * 60 * 60 * 24 * 365.25;
        diff = today - dob;
        diff = Math.floor(diff / one_year)
    }
    return diff
}

