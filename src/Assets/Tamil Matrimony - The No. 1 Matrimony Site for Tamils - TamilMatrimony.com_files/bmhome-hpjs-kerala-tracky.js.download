function showmenubar(block) {
    $(".navbg a").click(function(e) {
        e.stopPropagation()
    });
    $(".menublock").click(function(e) {
        e.stopPropagation()
    });
    $('#' + block).show();
    $('#' + block).prev().addClass('midalog-active').removeClass('midalog');
}

function hidemenubar(block) {
    $('#' + block).hide();
    $('#' + block).prev().addClass('midalog').removeClass('midalog-active');
}

function orgPasswordField(e, realField) {
    e.style.display = 'none';
    var realField = document.getElementById(realField);
    realField.style.display = 'inline';
    realField.value = '';
    realField.focus();
}

function temppasswordFieldBlur(e, fakeField) {
    if (e.value == "" || e.value == "Password") {
        e.style.display = 'none';
        var fakeField = document.getElementById(fakeField);
        fakeField.style.display = 'inline';
    }
}

function topnavlogincheck() {
    if (dgebid("ID").value == "Matrimony ID / Mobile No. / E-mail" || dgebid("ID").value == "" || dgebid("ID").value == "ID / E-mail") {
        $('.TEMPPASSWD1').removeClass('errorbg');
        $('#ID').addClass('errorbg').focus();
        return false;
    } else if (document.getElementsByTagName('input')[1].value == "Password" || document.getElementsByTagName('input')[1].value == "") {
        $('#ID').removeClass('errorbg');
        $('.TEMPPASSWD1').addClass('errorbg').focus();
        return false;
    }
}

