function qsformvalidate() {

	
	
	var quicksearchform = this.document.quicksearchform;

	if (IsEmpty(quicksearchform.STAGE, "text") || IsEmpty(quicksearchform.ENDAGE, "text")) 
	{
        alert("Please enter age range");
        quicksearchform.STAGE.focus();
        return false
    }
    if (quicksearchform.GENDER.value == 'F') {
        if (quicksearchform.STAGE.value < 18) {
            alert("Sorry, invalid age " + quicksearchform.STAGE.value + " (Min. age is 18. Max. age is 70).");
            return false
        }
        if (quicksearchform.ENDAGE.value < 18) {
            alert("Sorry, invalid age " + quicksearchform.ENDAGE.value +
                " (Min. age is 18. Max. age is 70).");
            return false
        }
    }
    if (quicksearchform.GENDER.value == 'M') {
        if (quicksearchform.STAGE.value < 21) {
            alert("Sorry, invalid age " + quicksearchform.STAGE.value + " (Min. age is 21. Max. age is 70).");
            return false
        }
        if (quicksearchform.ENDAGE.value < 21) {
            alert("Sorry, invalid age " + quicksearchform.ENDAGE.value + " (Min. age is 21. Max. age is 70).");
            return false
        }
    }
    if (parseInt(quicksearchform.ENDAGE.value) - parseInt(quicksearchform.STAGE.value) > 22) {
        alert('The difference between a partner\'s "From" and "To" age should not exceed 22 years.');
        quicksearchform.ENDAGE.focus();
        return false
    }
    if (quicksearchform.STAGE.value > 70 || quicksearchform.ENDAGE.value > 70) {
        alert("Maximum age allowed is 70.");
        return false
    }
    if (quicksearchform.STAGE.value > quicksearchform.ENDAGE.value) {
        alert("Please enter valid age range");
        return false
    }
    if (quicksearchform.STAGE.value == "" || quicksearchform.STAGE.value == null || quicksearchform.ENDAGE.value == "" || quicksearchform.ENDAGE.value == null) {
        alert("Maximum age allowed is 70.");
        return false
    }
    quicksearchform.action = "http://profile." +
        DOMAINARRAY["domainnameshort"] + "matrimony.com/search/searchres_ql.php?t=Q&randid=" + genNumbers() + "&gaact=HP&gasrc=QSRCH" + DOMAINARRAY["domainnameshort"].toUpperCase();
    quicksearchform.submit();
    return true
}

function validate1() {
    var MatriForm1 = this.document.MatriForm1;
    if (IsEmpty(MatriForm1.ID, "text")) {
        alert("Please enter the Matrimony ID / E-Mail");
        MatriForm1.ID.focus();
        return false
    }
    if (IsEmpty(MatriForm1.PASSWORD, "password")) {
        alert("Please enter the Password");
        MatriForm1.PASSWORD.focus();
        return false
    }
    return true
}

function navigate() {
    var menuIndex = document.formMenu.selectMenu.selectedIndex;
    var ind = document.formMenu.selectMenu.options[menuIndex].value;
    if (ind == 0) {
        alert("Please Choose Any Category");
        formMenu.selectMenu.focus();
        return false
    } else window.open(ind, "")
}

